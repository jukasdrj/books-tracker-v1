{
  "name": "openlibrary-search-worker",
  "version": "1.0.0",
  "description": "Optimized OpenLibrary search worker for authoritative author works discovery",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "tail": "wrangler tail --format pretty",
    "test:andy": "curl 'https://openlibrary-search-worker.YOUR_SUBDOMAIN.workers.dev/author/andy%20weir' | jq '.'",
    "test:health": "curl 'https://openlibrary-search-worker.YOUR_SUBDOMAIN.workers.dev/health' | jq '.'"
  },
  "keywords": [
    "openlibrary",
    "books",
    "search",
    "cloudflare-workers",
    "swiftdata",
    "author-bibliography"
  ],
  "author": "BooksTracker Team",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "wrangler": "^3.0.0"
  }
}